              </Sequence>
            </If.Then>
            <If.Else>
              <mtbwa:WriteBuildWarning Message="此专用生成的生成输出将不会复制到该放置位置，因为未设置 PrivateDropLocation。"/>
            </If.Else>
          </If>
        </If.Then>
      </If>
    </Sequence>
    <mtbwa:AgentScope DisplayName="在代理上运行" MaxExecutionTime="[AgentSettings.MaxExecutionTime]" MaxWaitTime="[AgentSettings.MaxWaitTime]" ReservationSpec="[AgentSettings.GetAgentReservationSpec()]">
