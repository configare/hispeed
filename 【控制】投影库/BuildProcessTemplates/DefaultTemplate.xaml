                                            <mtbwa:SetBuildProperties CompilationStatus="[Microsoft.TeamFoundation.Build.Client.BuildPhaseStatus.Failed]" DisplayName="Set CompilationStatus to Failed" PropertiesToSet="CompilationStatus" mtbwt:BuildTrackingParticipant.Importance="Low"/>
                                            <If Condition="[CreateWorkItem]" DisplayName="If CreateWorkItem" mtbwt:BuildTrackingParticipant.Importance="Low">
                                              <If.Then>
                                                <mtbwa:InvokeForReason DisplayName="为非搁置集生成任务创建工作项" Reason="Manual, IndividualCI, BatchedCI, Schedule, ScheduleForced, UserCreated">
                                                  <mtbwa:OpenWorkItem AssignedTo="[BuildDetail.RequestedFor]" Comment="[&quot;此工作项是由 TFS Build 在生成失败时创建的。&quot;]" CustomFields="[New Dictionary(Of String, String) From { {&quot;System.Reason&quot;, &quot;生成错误&quot;}, {&quot;Microsoft.VSTS.TCM.ReproSteps&quot;, &quot;使用 TFS Build 开始生成&quot;}, {&quot;优先级别&quot;, &quot;1&quot;}, {&quot;严重级别&quot;, &quot;1 - 严重&quot;} }]" DisplayName="创建工作项" Title="[String.Format(&quot;版本生成失败: {0}&quot;, BuildDetail.BuildNumber)]" Type="[&quot;Bug&quot;]"/>
                                                </mtbwa:InvokeForReason>
                                              </If.Then>
                                            </If>
                                            <Rethrow DisplayName="由于再次引发异常，因此生成将停止" mtbwt:BuildTrackingParticipant.Importance="Low"/>
                                          </Sequence>
                                        </ActivityAction>
                                      </Catch>
                                    </TryCatch.Catches>
                                  </TryCatch>
                                </ActivityAction>
                              </ForEach>
                            </If.Then>
                          </If>
                          <If Condition="[Not DisableTests]" DisplayName="If Not DisableTests" mtbwt:BuildTrackingParticipant.Importance="Low">
                            <If.Then>
                              <Sequence DisplayName="运行测试" mtbwt:BuildTrackingParticipant.Importance="Low">
