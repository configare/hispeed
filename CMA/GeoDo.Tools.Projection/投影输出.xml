<?xml version="1.0" encoding="utf-8"?>
<xml identify="projectionoutput">
  <!--原始文件(不包含路径)-->
  <OrbitFilename></OrbitFilename>
  <!--卫星-->
  <Satellite></Satellite>
  <!--传感器-->
  <Sensor></Sensor>
  <!--数据级别-->
  <Level></Level>
  <!--投影标识-->
  <ProjectionIdentify></ProjectionIdentify>
  <!--观测日期-->
  <ObservationDate></ObservationDate>
  <!--观测时间-->
  <ObservationTime></ObservationTime>
  <!--接收站-->
  <Station></Station>
  <!--文件的大小(字节)（原始轨道文件的大小）-->
  <Length></Length>
  <!--白天或者晚上数据【D:白天，N:晚上，X:表示未知】-->
  <DayOrNight></DayOrNight>
  <!--轨道圈号标识,和输入的上次圈号标识比较,如果大于指定的时间范围，则返回当前的观测日期，
  否则，返回上次圈号-->
  <OrbitIdentify></OrbitIdentify>
  <OutputFiles>
    <File>
      <!--输出文件名(不包含路径)-->
      <OutputFilename></OutputFilename>
      <!--缩略图文件-->
      <Thumbnail></Thumbnail>
      <!--辅助文件"***.hdr,***.info,..."-->
      <ExtendFiles></ExtendFiles>
      <!--文件属性[double],经纬度范围、分辨率-->
      <Envelope name="" minx="" maxx="" miny="" maxy=""/>
      <ResolutionX></ResolutionX>
      <ResolutionY></ResolutionY>
      <!--文件的大小(字节)-->
      <Length></Length>
    </File>
    <File></File>
  </OutputFiles>
  <!--投影返回消息-->
  <log>
    <loglevel>info|warning|error</loglevel>
    <loginfo>...</loginfo>
  </log>
</xml>